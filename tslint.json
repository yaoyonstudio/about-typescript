{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": [true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ],
      "semicolon": [true, "never"],
      "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        // Pending fix for shorthand property names.
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
